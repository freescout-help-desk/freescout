APP_NAME=FreeScout
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost:${FREESCOUT_WEB_PORT}

# WARP ports (registered)
FREESCOUT_WEB_PORT=8087
FREESCOUT_DB_PORT=3308
FREESCOUT_REDIS_PORT=6384
FREESCOUT_MAILHOG_HTTP_PORT=8026
FREESCOUT_MAILHOG_SMTP_PORT=1027

# Database (docker-compose defaults)
DB_CONNECTION=mysql
DB_HOST=mariadb
DB_PORT=3306
DB_DATABASE=freescout
DB_USERNAME=freescout
DB_PASSWORD=freescout

# Cache/Queue/Session
BROADCAST_DRIVER=log
CACHE_DRIVER=redis
QUEUE_CONNECTION=sync
SESSION_DRIVER=redis
SESSION_LIFETIME=120

REDIS_HOST=redis
REDIS_PASSWORD=null
REDIS_PORT=6379

# Mail
MAIL_MAILER=smtp
MAIL_HOST=mailhog
MAIL_PORT=${FREESCOUT_MAILHOG_SMTP_PORT}
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS=admin@example.com
MAIL_FROM_NAME="${APP_NAME}"

# AI
OLLAMA_HOST=http://host.docker.internal:11434
OLLAMA_MODEL=tinyllama

